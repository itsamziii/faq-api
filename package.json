{
    "name": "faq-backend",
    "version": "1.0.0",
    "description": "Assignment for BharatFD",
    "main": "dist/index.js",
    "scripts": {
        "dev": "nodemon --watch src -e ts --exec node --loader ts-node/esm src/index.ts",
        "start": "node --enable-source-maps dist/index.js",
        "build": "npm run clean && tsc --noEmit && swc ./src --out-dir ./dist --strip-leading-paths",
        "clean": "del-cli dist",
        "lint:fix": "eslint src/ --fix",
        "lint": "eslint src/",
        "format": "prettier --write src/",
        "test": "vitest run"
    },
    "type": "module",
    "author": "ItsAmziii",
    "license": "MIT",
    "devDependencies": {
        "@swc/cli": "^0.6.0",
        "@swc/core": "^1.10.12",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/jsonwebtoken": "^9.0.8",
        "@types/node": "^22.12.0",
        "@types/supertest": "^6.0.2",
        "del-cli": "^6.0.0",
        "eslint": "^9.19.0",
        "eslint-config-prettier": "^10.0.1",
        "nodemon": "^3.1.9",
        "pino-pretty": "^13.0.0",
        "prettier": "^3.4.2",
        "supertest": "^7.0.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.22.0",
        "vitest": "^3.0.4"
    },
    "dependencies": {
        "@vitalets/google-translate-api": "^9.2.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "ioredis": "^5.4.2",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.9.5",
        "pino": "^9.6.0",
        "zod": "^3.24.1"
    },
    "packageManager": "pnpm@9.15.4"
}